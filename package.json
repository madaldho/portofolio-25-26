    "@contentful/rich-text-types": "^17.2.5",
    "@fontsource/inter": "^5.2.8",
    "@fontsource/jetbrains-mono": "^5.2.8",
    "@fontsource/space-grotesk": "^5.2.10",
    "@tailwindcss/vite": "^4.1.18",
    "astro": "^5.16.9",
    "astro-opengraph-images": "^1.14.0",
    "contentful": "^11.10.2",
    "marked": "^17.0.1",
    "tailwindcss": "^4.1.18",
    "tw-to-css": "^0.0.12",
    "zod": "^3.25.76"
  },
  "devDependencies": {
