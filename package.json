{
  "name": "portfolio",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "astro dev",
    "build": "node scripts/optimize-build.js",
    "build:basic": "astro build",
    "preview": "astro preview",
    "astro": "astro",
    "setup:images": "node scripts/setup-images.js",
    "test": "vitest",
    "build:analyze": "astro build --analyze",
    "lighthouse": "lighthouse http://localhost:4321 --output html --output-path ./lighthouse-report.html",
