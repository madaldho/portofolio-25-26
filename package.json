    "contentful": "^11.10.1",
    "marked": "^17.0.1",
    "tailwindcss": "^4.1.18",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@types/node": "^25.0.2",
    "fast-check": "^4.4.0",
    "jsdom": "^25.0.1",
